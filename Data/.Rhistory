euromat = as.matrix(eurodist)
euromat[1:5, 1:5]
mds = cmdscale(eurodist, k = 2)
mdsd = as.data.frame(mds)
ggplot(data = mdsd) +
geom_point(mapping = aes(x = V1, y = V2)) +
ggrepel::geom_label_repel (
aes(x = V1, y = V2, label = row.names(mdsd))
)
